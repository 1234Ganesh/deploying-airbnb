<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" ></script>
  <title>airbub page</title>
 
</head>
<body>
  <%- include('../partial/header') %>
 



  <div class="container shadow m-5">
    <div class="row d-flex justify-content-center">
      <div class="col-12 mt-4 mb-4">
        <h4 class="text-secondary text-center">Here are our Registered Homes</h4>

      </div>
      <% registeredHomes.forEach(home=>{%>

    
        <div class="col-md-4">
          <div class="card shadow-sm mb-4">
            <img src="<%= home.photoUrl %>" style="width: 100%; height: 200px;" alt="Product Image">
            <div class="card-body">
              <h5><%= home.houseName %></h5>
              <p class="text-primary"><%= home.location %></p>
              <h5>Rs <%= home.price %> /Night</h5>
              <p class="text-danger"><%= home.rating %></p>
           
             <button class="btn btn-primary mb-1"> <a href="/homes/<%= home._id %>" class="text-white text-decoration-none mb-2">Details</a></button>
             <%- include('../partial/favourite',{home:home}) %>
              
            </div>
          </div>
        </div>
   <% })%>
    </div>
  </div>


</body>
 
</body>
</html>